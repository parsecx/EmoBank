        <script>

            $(document).ready(function () {

$('tr').each( function () { 
    $(this).children('td:gt(0)').filter(function() {
        return this.innerHTML.match(/^[0-9\s\.,]+$/);
    }).css('text-align','right')
});
                $(document).on('focus', '.myDate', function () {
                    $(this).datepicker(
                        {
                            format: "yyyy/mm/dd"
                        }
                        );
                });
				
			
						var src1 = "../images/Logo4.png";
						var src2 = "../assets/images/logo.png";
						
						$(".menub").click(function(){
						   var src = $(".navbar-brand-logo").attr('src'); 
						   if(src == src1){$(".navbar-brand-logo").attr('src',src2);$('.page').css('margin-left','266px');$('.site-menubar').css('width','266px');}
						   else{$(".navbar-brand-logo").attr('src',src1);$('.page').css('margin-left','0px');	$('.site-menubar').css('width','0px');}
						});


